{
  "manifest_version": 3,
  "name": "LinkedIn Formatter",
  "version": "1.1.0",
  "description": "Adds additional buttons to format the text while creating a post in LinkedIn.",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/feed/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "48": "icons/icon.svg"
  }
}